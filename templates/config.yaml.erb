apiVersion: kubeadm.k8s.io/v1alpha3
apiEndpoint:
  advertiseAddress: <%= @kube_api_advertise_address %>
  bindPort: 6443
bootstrapTokens:
- groups:
  - system:bootstrappers:kubeadm:default-node-token
  token: <%= @token %>
  ttl: 0s
  usages:
  - signing
  - authentication
kind: InitConfiguration
nodeRegistration:
  criSocket: /var/run/dockershim.sock
  name: <%= @node_label %>
  taints:
  - effect: NoSchedule
    key: node-role.kubernetes.io/master
---
apiServerCertSANs:
<%- if @apiserver_cert_extra_sans -%>
<% @apiserver_cert_extra_sans.each do |san| -%>
- <%= san %>
<% end -%>
<% end -%>
apiServerExtraArgs:
  endpoint-reconciler-type: lease
<% @apiserver_extra_arguments.each do |arg| -%>
  <%= arg %>
<% end -%>
apiVersion: kubeadm.k8s.io/v1alpha3
etcd:
  external:
    caFile: /etc/kubernetes/pki/etcd/ca.crt
    certFile: /etc/kubernetes/pki/etcd/client.crt
    endpoints:
    <%- @etcd_peers.each do |peer| -%>
    - https://<%= peer %>:2379
    <%- end -%>
    keyFile: /etc/kubernetes/pki/etcd/client.key
certificatesDir: /etc/kubernetes/pki
controlPlaneEndpoint: ""
<%- if @feature_gates -%>
featureGates:
<% @feature_gates.each do |feature, value| -%>
  <%= feature %>: <%= value %>
<% end -%>
<% end -%>
networking:
  dnsDomain: cluster.local
  podSubnet: ""
  serviceSubnet: "<%= @service_cidr %>"
controllerManagerExtraArgs:
  address: "<%= @controller_manager_address %>"
  allocate-node-cidrs: "<%= @cni_pod_cidr_allocate %>"
  cluster-cidr: "<%= @cni_pod_cidr %>"
  node-cidr-mask-size: "<%= @cni_pod_cidr_mask %>"
  service-cluster-ip-range: "<%= @service_cidr %>"
imageRepository: k8s.gcr.io
kind: ClusterConfiguration
kubernetesVersion: <%= @kubernetes_version.split('-').first %>
networking:
schedulerExtraArgs:
  address: "<%= @scheduler_address %>"
unifiedControlPlaneImage: ""
---
apiVersion: kubeproxy.config.k8s.io/v1alpha1
bindAddress: 0.0.0.0
clientConnection:
  acceptContentTypes: ""
  burst: 10
  contentType: application/vnd.kubernetes.protobuf
  kubeconfig: /var/lib/kube-proxy/kubeconfig.conf
  qps: 5
clusterCIDR: "<%= @cni_pod_cidr %>"
configSyncPeriod: 15m0s
conntrack:
  max: null
  maxPerCore: 32768
  min: 131072
  tcpCloseWaitTimeout: 1h0m0s
  tcpEstablishedTimeout: 24h0m0s
enableProfiling: false
healthzBindAddress: 0.0.0.0:10256
hostnameOverride: ""
iptables:
  masqueradeAll: false
  masqueradeBit: 14
  minSyncPeriod: 0s
  syncPeriod: 30s
ipvs:
  excludeCIDRs: null
  minSyncPeriod: 0s
  scheduler: ""
  syncPeriod: 30s
kind: KubeProxyConfiguration
metricsBindAddress: 127.0.0.1:10249
mode: ipvs
nodePortAddresses: null
oomScoreAdj: -999
portRange: ""
resourceContainer: /kube-proxy
udpIdleTimeout: 250ms
